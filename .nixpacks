[phases]
# Instalar dependencias de Django (backend)
install_backend = "pip install -r back/requirements.txt"

# Instalar dependencias de React (frontend)
install_frontend = "cd front && npm install"

# Construir el frontend
build_frontend = "cd front && npm run build"



# Iniciar el servidor de Django
start = "cd back && python manage.py runserver 0.0.0.0:8000"
